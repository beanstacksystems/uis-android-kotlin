<?xml version="1.0" encoding="utf-8"?>


<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item  android:height="30dp" android:width="30dp" >
        <shape android:shape="oval" >
            <corners android:radius="5dp" />
            <solid android:color="#FFFFFF"  />
            <stroke android:color="#D8D8D8" android:width="1dp"/>
            <size android:height="30dp" android:width="30dp" />
        </shape>
    </item>
    <item android:height="20dp" android:width="20dp" android:gravity="center">
        <shape android:shape="oval">
            <corners android:radius="5dp" />
            <solid android:color="#97294D" />
            <stroke android:color="#D8D8D8" android:width="0dp"/>

        </shape>
    </item>


</layer-list>