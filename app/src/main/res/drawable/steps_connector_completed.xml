<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item >
        <shape android:shape="rectangle" >
            <corners android:radius="1dp" />
            <solid android:color="#FFFFFF"  />
            <stroke android:color="#D8D8D8" android:width="1dp"/>
            <size android:height="10dp" android:width="30dp" />
        </shape>
    </item>

    <item android:height="7dp"  android:gravity="center">
        <shape android:shape="rectangle">
            <corners android:radius="1dp" />
            <solid android:color="#97294D" />
            <stroke android:color="#D8D8D8" android:width="0dp"/>
        </shape>
    </item>
</layer-list>